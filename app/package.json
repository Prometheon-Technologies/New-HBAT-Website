        "autoprefixer": "^10.4.18",
        "babel-preset-solid": "^1.8.16",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "solid-form-handler": "^1.2.2",
        "solid-headless": "^0.13.1",
        "solid-i18n": "^1.1.0",
        "solid-icons": "^1.1.0",
        "solid-js": "^1.8.16",
        "solid-spinner": "^0.2.0",
        "solid-transition-group": "^0.2.3",

{
    "name": "app",
    "version": "0.1.0",
    "private": true,
    "description": "A SolidJS project for EyeTrack VR",
    "author": "DaOfficialWizard",
    "license": "MIT",
    "type": "module",
    "babelMacros": {
        "twin": {
            "preset": "solid"
        }
    },
    "scripts": {
        "dev": "vinxi dev --config ./vite.config.ts --root ./ --port 5487 --host",
        "build": "vinxi build",
        "start": "vinxi start --port 5478 --host",
        "docs": "jsdoc -c jsdoc.conf.json",
        "lint": "eslint --ext .js,.ts,.jsx,.tsx src",
        "format": "pnpm run lint --fix & pnpm prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
        "update-deps": "pnpm up -Li",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@babel/core": "^7.24.3",
        "@babel/preset-env": "^7.24.3",
        "@emotion/serialize": "^1.1.2",
        "@kobalte/tailwindcss": "^0.9.0",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@types/node": "^20.11.30",
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "babel-loader": "^9.1.3",
        "babel-plugin-macros": "^3.1.0",
        "daisyui": "^4.7.3",
        "eslint": "^8.57.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-autofix": "^1.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-solid": "^0.13.2",
        "postcss": "^8.4.37",
        "postcss-import": "^16.0.1",
        "prettier": "^3.2.5",
        "tailwind-merge": "^2.2.2",
        "tailwindcss-animate": "^1.0.7",
        "tailwindcss-brand-colors": "^0.3.6",
        "tailwindcss-debug-screens": "^2.2.1",
        "twin.macro": "^3.4.0",
        "typescript": "^5.4.2",
        "vinxi": "^0.3.10"
    },
    "dependencies": {
        "@kobalte/core": "^0.12.6",
        "@solid-primitives/i18n": "^2.1.1",
        "@solid-primitives/map": "^0.4.11",
